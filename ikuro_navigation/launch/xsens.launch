<?xml version="1.0"?>
<launch>
<!--  xsens package -->
  	<node pkg="xsens_driver" type="mtnode.py" name="xsens_driver" output="screen" >
        <param name="frame_id" type="string" value="imu_link" />
		<param name="port" value="/dev/xsens" />
        <param name="baudrate" value="115200" />
	</node>

	<node pkg="tf" type="static_transform_publisher" name="xsens_tf_broadcaster" args="-0.01 0 0 0 0 0 base_link imu_link 10" />


</launch>
